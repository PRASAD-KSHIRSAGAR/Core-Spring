<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"https://www.springframework.org/dtd/spring-beans-2.0.dtd">
	<beans>
		<bean id="ei" class="com.springcoreApp.Employee">
			<property name="id" value="1"/>
			<property name="Name" value="Prasad"/>
			<property name="Marks" value="300"/>
		</bean>
		
		<bean id="t1" class="com.springcoreApp.Test">
			<constructor-arg value="Ram" type="java.lang.String" index="0"></constructor-arg>
			<constructor-arg value="ram@gmail.com" type="java.lang.String" index="1"></constructor-arg>
			<constructor-arg value="10000" type="int" index="2"></constructor-arg>
		</bean>
	</beans>